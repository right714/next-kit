{
  "name": "kit-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "sb": "start-storybook -p 6006",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint --fix && tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:update": "vitest run --update",
    "test:ui": "vitest --ui",
    "prettier": "prettier .",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "format": "npm run prettier:fix && npm run stylelint:fix",
    "new:page": "hygen generator page",
    "new:ui": "hygen generator ui-component",
    "new:feature": "hygen generator feature-component",
    "npm:outdated": "ncu",
    "npm:update": "ncu -u && npm install"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@testing-library/react": "^13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "classnames": "^2.3.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.2",
    "hygen": "^6.2.11",
    "jotai": "^1.10.0",
    "jsdom": "^20.0.3",
    "next-seo": "^5.14.1",
    "npm-check-updates": "^16.4.3",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-order": "^5.0.0",
    "swr": "^1.3.0",
    "tailwindcss": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "engines": {
    "node": "16.x"
  }
}
